<div class="container">
  <h2>ユーザー一覧</h2>
  <% @users.each do |user| %>
      <div class="container-child col-md-2 col-sm-4 col-xs-6 ">
        <p>ユーザー名：<%= link_to user.name, user %></p>
        <p><%= profile_img(user) %></p>
        <p>Eメール：<%= user.email %></p><br>
        <%= render 'follow_form', user: user %>
      </div>
  <% end %>
</div>
